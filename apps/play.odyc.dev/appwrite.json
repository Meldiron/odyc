{
	"projectId": "odyc-js",
	"endpoint": "https://fra.cloud.appwrite.io/v1",
	"projectName": "Odyc.js",
	"settings": {
		"services": {
			"account": true,
			"avatars": false,
			"databases": true,
			"locale": false,
			"health": false,
			"storage": false,
			"teams": false,
			"users": true,
			"sites": true,
			"functions": false,
			"graphql": true,
			"messaging": false
		},
		"auth": {
			"methods": {
				"jwt": true,
				"phone": false,
				"invites": false,
				"anonymous": true,
				"email-otp": true,
				"magic-url": false,
				"email-password": false
			},
			"security": {
				"duration": 31536000,
				"limit": 0,
				"sessionsLimit": 10,
				"passwordHistory": 0,
				"passwordDictionary": false,
				"personalDataCheck": false,
				"sessionAlerts": false,
				"mockNumbers": []
			}
		}
	},
	"databases": [
		{
			"$id": "main",
			"name": "main",
			"enabled": true
		}
	],
	"collections": [
		{
			"$id": "games",
			"$permissions": ["create(\"users\")"],
			"databaseId": "main",
			"name": "games",
			"enabled": true,
			"documentSecurity": true,
			"attributes": [
				{
					"key": "name",
					"type": "string",
					"required": true,
					"array": false,
					"size": 64,
					"default": null
				},
				{
					"key": "thumbnailFileId",
					"type": "string",
					"required": false,
					"array": false,
					"size": 32,
					"default": null
				},
				{
					"key": "ownerProfileId",
					"type": "string",
					"required": true,
					"array": false,
					"size": 32,
					"default": null
				},
				{
					"key": "description",
					"type": "string",
					"required": false,
					"array": false,
					"size": 500,
					"default": null
				},
				{
					"key": "howToPlay",
					"type": "string",
					"required": false,
					"array": false,
					"size": 500,
					"default": null
				},
				{
					"key": "tags",
					"type": "string",
					"required": false,
					"array": true,
					"size": 32,
					"default": null
				},
				{
					"key": "slug",
					"type": "string",
					"required": false,
					"array": false,
					"size": 32,
					"default": null
				},
				{
					"key": "private",
					"type": "boolean",
					"required": true,
					"array": false,
					"default": null
				},
				{
					"key": "collaboratorProfileIds",
					"type": "string",
					"required": true,
					"array": false,
					"size": 32,
					"default": null
				}
			],
			"indexes": [
				{
					"key": "slug",
					"type": "key",
					"status": "available",
					"attributes": ["slug"],
					"orders": ["ASC"]
				},
				{
					"key": "ownerProfileId",
					"type": "key",
					"status": "available",
					"attributes": ["ownerProfileId"],
					"orders": ["ASC"]
				}
			]
		},
		{
			"$id": "communityHighlights",
			"$permissions": ["read(\"users\")"],
			"databaseId": "main",
			"name": "communityHighlights",
			"enabled": true,
			"documentSecurity": false,
			"attributes": [
				{
					"key": "gameId",
					"type": "string",
					"required": true,
					"array": false,
					"size": 32,
					"default": null
				}
			],
			"indexes": []
		},
		{
			"$id": "profiles",
			"$permissions": ["create(\"users\")", "read(\"users\")"],
			"databaseId": "main",
			"name": "profiles",
			"enabled": true,
			"documentSecurity": true,
			"attributes": [
				{
					"key": "name",
					"type": "string",
					"required": true,
					"array": false,
					"size": 32,
					"default": null
				},
				{
					"key": "avatarPixels",
					"type": "string",
					"required": false,
					"array": false,
					"size": 255,
					"default": null
				}
			],
			"indexes": []
		}
	]
}
